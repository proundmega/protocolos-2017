;
; BIND data file for local loopback interface
;
$TTL	10
@	IN	SOA	manijapones.com. root.manijapones.com. (
			      1		; Serial
			     10		; Refresh
			     10		; Retry
			     30		; Expire
			     10 )	; Negative Cache TTL
;
@	IN	NS	manijapones.com.
@	IN	A	192.168.0.18
@	IN	AAAA	2001:db8:2::1

@	IN	MX   0  manijapones.com.

www	IN	A	192.168.0.20
www	IN	AAAA	2001:db8:2::2

uah	IN	CNAME	manijapones.com.
