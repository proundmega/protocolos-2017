{
  "consul": "consul:8500",
  "logging": {
    "level": "INFO",
    "format": "default",
    "output": "stdout"
  },
  "services": [
    {
      "name": "app",
      "port": 8080,
      "health": [
        "pidof",
        "java"
        ],
      "interfaces": [
        "eth0"
      ],
      "poll": 10,
      "ttl": 30,
      "tags": ["1.1"]
    }
  ],
  "backends": [
    {
      "name": "redis",
      "poll": 30,
      "onChange": "/usr/local/bin/reload-app.sh"
    }
  ]
}
