{
  "consul": "consul:8500",
  "preStart": "/usr/local/bin/preStart-script.sh",
  "logging": {
    "level": "INFO",
    "format": "default",
    "output": "stdout"
  },
  "services": [
    {
      "name": "app",
      "port": 8080,
      "health": [
        "pidof",
        "java"
        ],
      "interfaces": [
        "eth0"
      ],
      "poll": 10,
      "ttl": 30,
      "tags": ["tag1"]
    }
  ],
  "backends": [
    {
      "name": "redis",
      "poll": 30,
      "onChange": "/usr/local/bin/reload-app.sh"
    }
  ],
  "tasks": [
    {
      "name": "task",
      "command": ["/usr/local/bin/task.sh","arg1"],
      "frequency": "1500ms",
      "timeout": "100ms"
    }
  ]
}
